<?xml version="1.0"?>
<robot name="holly">

    <link name="/base_footprint">
        <visual>
            <geometry>
                <box size="0.64 0.35 0.005"/>
            </geometry>
            <material name="grey" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="base_to_footprint" type="fixed">
        <parent link="/base_footprint" />
        <child link="/base_link" />
        <origin rpy="0 0 0" xyz="0 0 0.21" />
    </joint>

    <link name="/base_link">
        <visual>
            <geometry>
                <box size="0.505 0.27 0.02"/>
            </geometry>
            <material name="grey" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <link name="/bogie_rear">
        <visual>
            <geometry>
                <box size="0.38 0.02 0.02" />
            </geometry>
            <material name="grey" />
            <origin rpy="0 0 1.57" xyz="0 0 0" />
        </visual>
    </link>

    <link name="/bogie_left">
        <visual>
            <geometry>
                <box size="0.32 0.02 0.02" />
            </geometry>
            <material name="grey" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <link name="/bogie_right">
        <visual>
            <geometry>
                <box size="0.32 0.02 0.02" />
            </geometry>
            <material name="grey" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="base_to_bogie_rear" type="revolute">
        <parent link="/base_link" />
        <child link="/bogie_rear" />
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="-0.27 0 -0.03" />
        <limit lower="-0.261" upper="0.261" effort="1000" velocity="0.5" />
    </joint>

    <joint name="base_to_bogie_left" type="revolute">
        <parent link="/base_link" />
        <child link="/bogie_left" />
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.12 0.17 -0.03" />
        <limit lower="-0.261" upper="0.261" effort="1000" velocity="0.5" />
    </joint>

    <joint name="base_to_bogie_right" type="revolute">
        <parent link="/base_link" />
        <child link="/bogie_right" />
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.12 -0.17 -0.03" />
        <limit lower="-0.261" upper="0.261" effort="1000" velocity="0.5" />
    </joint>

    <joint name="bogie_left_to_motor1" type="revolute">
        <parent link="/bogie_left" />
        <child link="/wheel1" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 0 0" xyz="0.1 0.0 -0.08" />
        <limit lower="-0.785" upper="0.785" effort="1000" velocity="0.5" />
    </joint>

    <joint name="bogie_left_to_motor2" type="fixed">
        <parent link="/bogie_left" />
        <child link="/wheel2" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 0 0" xyz="-0.1 0.0 -0.08" />
    </joint>

    <joint name="bogie_right_to_motor3" type="revolute">
        <parent link="/bogie_right" />
        <child link="/wheel3" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 3.14159 0" xyz="0.1 0 -0.08" />
        <limit lower="-0.785" upper="0.785" effort="1000" velocity="0.5" />
    </joint>

    <joint name="bogie_right_to_motor4" type="fixed">
        <parent link="/bogie_right" />
        <child link="/wheel4" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 3.14159 0" xyz="-0.1 0 -0.08" />
    </joint>

    <joint name="bogie_rear_to_motor5" type="revolute">
        <parent link="/bogie_rear" />
        <child link="/wheel5" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 0 0" xyz="0 0.17 -0.08" />
        <limit lower="-0.785" upper="0.785" effort="1000" velocity="0.5" />
    </joint>

    <joint name="bogie_rear_to_motor6" type="revolute">
        <parent link="/bogie_rear" />
        <child link="/wheel6" />
        <axis xyz="0 1 0"/>
        <origin rpy="1.570895 3.14159 0" xyz="0 -0.17 -0.08" />
        <limit lower="-0.785" upper="0.785" effort="1000" velocity="0.5" />
    </joint>

    <link name="/chassis_height_sensor">
         <visual>
            <geometry>
                <box size="0.05 0.05 0.05" />
            </geometry>
            <material name="red" />
        </visual>
    </link>

    <joint name="base_to_chassis_height_sensor" type="fixed">
        <parent link="/base_link" />
        <child link="/chassis_height_sensor" />
        <origin xyz="0 0.0 0.0" rpy="0 1.570895 0" />
    </joint>

    <link name="/laserscan_frame"></link>

    <joint name="base_to_laserscan_sensor" type="fixed">
        <parent link="/base_link" />
        <child link="/laserscan_frame" />
        <origin xyz="0.25 0.0 0.0" rpy="0 0 0" />
    </joint>


    <joint name="base_to_kinect_sensor" type="fixed">
        <parent link="/base_link" />
        <child link="/kinect_link" />
        <origin xyz="0.25 0.0 0.38" rpy="0 0.349066 0" />
    </joint>

    <link name="/kinect_link">
        <visual>
            <geometry>
                <box size="0.064 0.28 0.0381" />
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <joint name="kinect_depth_joint" type="fixed">
        <origin xyz="0 -0.0125 0" rpy="0 0 0" />
        <parent link="/kinect_link" />
        <child link="/kinect_depth_frame" />
    </joint>

    <link name="/kinect_depth_frame">

    </link>

    <joint name="depth_optical_joint" type="fixed">
        <origin xyz="0 0.025 0" rpy="-1.570795 0 -1.570795" />
        <parent link="/kinect_depth_frame" />
        <child link="/kinect_depth_optical_frame" />
    </joint>

    <link name="/kinect_depth_optical_frame">

    </link>


    <link name="/wheel1">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <link name="/wheel2">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <link name="/wheel3">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <link name="/wheel4">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <link name="/wheel5">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>

    <link name="/wheel6">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.06"/>
            </geometry>
            <material name="black" />
        </visual>
    </link>


    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

</robot>